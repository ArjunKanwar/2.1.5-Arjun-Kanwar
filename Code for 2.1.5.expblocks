\n\n// START EXP MACROS\n#define waitUntil(condition)                                                   \\\n  do {                                                                         \\\n    wait(5, msec);                                                             \\\n  } while (!(condition))\n\n#define repeat(iterations)                                                     \\\n  for (int iterator = 0; iterator < iterations; iterator++)\n// END EXP MACROS\n\n\n// Robot configuration code.\ninertial BrainInertial = inertial();\nmotor Motor1 = motor(PORT1, false);\n\nmotor Motor5 = motor(PORT5, true);\n\n\n\n\n// Generated code.\n\n// Include the EXP Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\nint Brain_precision = 0, Console_precision = 0;\n\nfloat myVariable, Move, Col, Row;\n\nbool DEBUG;\n\nfloat Grid[5][5];\n\nevent RT = event();\nevent message1 = event();\nevent LT = event();\nevent FWD = event();\n\n// \"when I receive RT\" hat block\nvoid onevent_RT_0() {\n  if (DEBUG) {\n    Brain.Screen.newLine();\n    Brain.Screen.print(\"LEFT\");\n  }\n  else {\n    Motor5.spinFor(reverse, Move, degrees, true);\n  }\n}\n\n// \"when I receive RT\" hat block\nvoid onevent_RT_1() {\n  if (DEBUG) {\n    Brain.Screen.newLine();\n    Brain.Screen.print(\"LEFT\");\n  }\n  else {\n    Motor1.spinFor(forward, Move, degrees, true);\n  }\n}\n\n// \"when started\" hat block\nint whenStarted1() {\n  Col = 1.0;\n  Row = 1.0;\n  Move = 233.0;\n  DEBUG = false;\n  Grid[0][0] = 2.0;\n  Grid[0][1] = 0.0;\n  Grid[0][2] = 0.0;\n  Grid[0][3] = 0.0;\n  Grid[0][4] = 0.0;\n  Grid[1][0] = 1.0;\n  Grid[1][1] = 0.0;\n  Grid[1][2] = 0.0;\n  Grid[1][3] = 0.0;\n  Grid[1][4] = 0.0;\n  Grid[2][0] = 0.0;\n  Grid[2][1] = 0.0;\n  Grid[2][2] = 0.0;\n  Grid[2][3] = 0.0;\n  Grid[2][4] = 0.0;\n  Grid[3][0] = 0.0;\n  Grid[3][1] = 0.0;\n  Grid[3][2] = 0.0;\n  Grid[3][3] = 1.0;\n  Grid[3][4] = 1.0;\n  Grid[4][0] = 0.0;\n  Grid[4][1] = 1.0;\n  Grid[4][2] = 0.0;\n  Grid[4][3] = 0.0;\n  Grid[4][4] = 3.0;while ((Row < 6.0)) {\n    Col = 1.0;\n    while ((Col < 6.0)) {\n      if (Grid[static_cast<int>(Row) - 1][static_cast<int>(Col) - 1] == 0.0) {\n        Brain.Screen.setPenColor(blue);\n        Brain.Screen.print(\"!\");\n      } else if (Grid[static_cast<int>(Row) - 1][static_cast<int>(Col) - 1] == 1.0) {\n        Brain.Screen.setPenColor(red);\n        Brain.Screen.print(\"*\");\n      } else if (Grid[static_cast<int>(Row) - 1][static_cast<int>(Col) - 1] == 2.0) {\n        Brain.Screen.setPenColor(green);\n        Brain.Screen.print(\"#\");\n      } else {\n        Brain.Screen.setPenColor(orange);\n        Brain.Screen.print(\"#\");\n      }\n      Col = Col + 1.0;\n    wait(5, msec);\n    }\n    Brain.Screen.newLine();\n    Row = Row + 1.0;\n  wait(5, msec);\n  }\n  repeat(2) {\n    FWD.broadcast();\n    wait(2.0, seconds);\n    wait(5, msec);\n  }\n  RT.broadcast();\n  wait(2.0, seconds);\n  repeat(4) {\n    FWD.broadcast();\n    wait(2.0, seconds);\n    wait(5, msec);\n  }\n  LT.broadcast();\n  wait(2.0, seconds);\n  repeat(2) {\n    FWD.broadcast();\n    wait(2.0, seconds);\n    wait(5, msec);\n  }\n  return 0;\n}\n\n// \"when I receive LT\" hat block\nvoid onevent_LT_0() {\n  if (DEBUG) {\n    Brain.Screen.newLine();\n    Brain.Screen.print(\"LEFT\");\n  }\n  else {\n    Motor1.spinFor(reverse, Move, degrees, true);\n  }\n}\n\n// \"when I receive LT\" hat block\nvoid onevent_LT_1() {\n  if (DEBUG) {\n    Brain.Screen.newLine();\n    Brain.Screen.print(\"LEFT\");\n  }\n  else {\n    Motor5.spinFor(forward, Move, degrees, true);\n  }\n}\n\n// \"when I receive FWD\" hat block\nvoid onevent_FWD_0() {\n  if (DEBUG) {\n    Brain.Screen.newLine();\n    Brain.Screen.print(\"LEFT\");\n  }\n  else {\n    Motor5.spinFor(forward, 412.56, degrees, true);\n  }\n}\n\n// \"when I receive FWD\" hat block\nvoid onevent_FWD_1() {\n  if (DEBUG) {\n    Brain.Screen.newLine();\n    Brain.Screen.print(\"LEFT\");\n  }\n  else {\n    Motor1.spinFor(forward, 412.56, degrees, true);\n  }\n}\n\n\nint main() {\n  // register event handlers\n  RT(onevent_RT_0);\n  RT(onevent_RT_1);\n  LT(onevent_LT_0);\n  LT(onevent_LT_1);\n  FWD(onevent_FWD_0);\n  FWD(onevent_FWD_1);\n\n  wait(15, msec);\n  // post event registration\n\n  // set default print color to black\n  printf(\"\\033[30m\");\n\n  // Wait for ADI sensors to initialize\n  wait(100, msec);\n\n\n  whenStarted1();\n}","target":"Physical"}